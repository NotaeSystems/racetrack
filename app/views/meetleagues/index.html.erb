<%if @league%>
<h1> <%= link_to @league.name, @league %></h1>
<%end%>
<h1>Meets</h1>

<table class='table table-striped table-bordered table-condensed'>


<% @meetleagues.each do |meetleague| %>
  <tr>
    <td><%= link_to meetleague.meet.name, meet_path(:id => meetleague.meet_id), :class => 'btn' %></td>
    <td><%= link_to "League Rankings", rankings_path(:league_id => @league.id, :meet_id => meetleague.meet_id), :class => 'btn' %></td>
    <td><%= meetleague.league_id %></td>
    <td><%= link_to 'Show', meetleague %></td>
    <td><%= link_to 'Edit', edit_meetleague_path(meetleague) %></td>
    <td><%= link_to 'Destroy', meetleague, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meetleague', new_meetleague_path %>
