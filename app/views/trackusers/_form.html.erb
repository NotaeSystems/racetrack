
<%= simple_form_for(@trackuser) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nickname %>
    <%= f.select :status, ['Member', 'Pending', 'Removed', 'Banned', 'Manager'] %>


  </div>

  <div class="form-actions">
    <%= f.button :submit, 'Update Member' %>
  </div>
<% end %>
