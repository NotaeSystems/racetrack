<%= simple_form_for(@card) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.hidden_field :meet_id , :value => @card.meet_id%>
    <%= f.input :open %>
    <%= f.input :description %>
    <%= f.input :completed %>
    <%= f.input :completed_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
